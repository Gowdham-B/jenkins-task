pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Code fetched from GitHub'
            }
        }

        stage('Build') {
            steps {
                echo 'Building project from Git'
            }
        }

        stage('Test') {
            steps {
                echo 'Testing project from Git'
            }
        }
    }
}
